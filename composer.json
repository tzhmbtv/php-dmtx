{
    "name": "tzhmbtv/php-dmtx",
    "description": "Datamatrix reader/writer based on libdmtx with support for PHP 8.x",
    "type": "library",
    "keywords": ["datamatrix", "dmtx", "barcode", "libdmtx"],
    "homepage": "https://github.com/tzhmbtv/php-dmtx",
    "require": {
        "php": ">=8.2",
        "symfony/process": "~3.4||~4.0||~5.0||~6.0||~7.0",
        "symfony/options-resolver": "~3.4||~4.0||~5.0||~6.0||~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-0": { "Dmtx": "src/" }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Tachoire",
            "email": "pierre.tachoire@gmail.com",
            "role": "Original Author"
        },
        {
            "name": "Elnur Tazhimbetov",
            "email": "elnur9924@gmail.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/tzhbmtv/php-dmtx/issues",
        "source": "https://github.com/tzhbmtv/php-dmtx"
    },
    "replace": {
        "ptachoire/php-dmtx": "self.version"
    }
}
